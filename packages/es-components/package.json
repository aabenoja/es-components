{
  "name": "es-components",
  "version": "17.0.7",
  "description": "React components built for Exchange Solutions products",
  "repository": "https://github.com/wtw-im/es-components",
  "main": "lib/index.js",
  "browser": "es6/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:es5 && npm run build:es6 && npm run webpack",
    "build:es5": "babel src -d lib --only src --ignore **/*.specs.js",
    "build:es6": "cross-env BABEL_ENV=es6 babel src -d es6 --only src --ignore **/*.specs.js",
    "ci": "npm run lint && npm test",
    "lint": "eslint src",
    "prepare": "npm run build",
    "prettify": "prettier --single-quote --write \"src/**/*.js\"",
    "styleguide-build": "styleguidist build",
    "start": "styleguidist server",
    "test": "jest",
    "webpack": "webpack --config webpack-components-bundle.config.js"
  },
  "author": "Willis Towers Watson - Individual Marketplace",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "./config/jest-shims.js",
      "./config/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "es6",
      "lib"
    ],
    "testRegex": "specs\\.js$",
    "testURL": "http://localhost/"
  },
  "prettier": {
    "singleQuote": true
  },
  "peerDependencies": {
    "react": ">=0.14",
    "styled-components": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-preset-wtw-im": "^2.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "es-components-via-theme": "file:../es-components-via-theme",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-exchange-solutions": "^7.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^0.9.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mockdate": "^2.0.1",
    "prettier": "^1.14.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-styleguidist": "^7.3.8",
    "react-test-renderer": "^16.3.0",
    "regenerator-runtime": "^0.12.1",
    "request": "^2.88.0",
    "style-loader": "^0.13.1",
    "styled-components": "^3.4.6",
    "styled-enzyme": "^0.1.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "prop-types": "^15.6.1",
    "react-animate-height": "^2.0.4",
    "react-datepicker": "^1.6.0",
    "react-overlays": "^0.8.3",
    "react-popper": "^0.10.4",
    "react-text-mask": "5.4.1",
    "react-transition-group": "^2.2.1",
    "text-mask-addons": "3.7.2",
    "tinycolor2": "^1.4.1",
    "uncontrollable": "^6.0.0"
  }
}
